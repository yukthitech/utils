<testData xmlns:s="http://autox.yukthitech.com/steps" xmlns:wrap="http://xmlbeanparser.yukthitech.com/wrap" xmlns:f="http://autox.yukthitech.com/functions">
	<testSuite name="testSuiteData">
		<testCase name="dataSetupFail">
			<description>
				Validates the use case when data setup fails.
			</description>
			
			<list-data-provider name="dataString">
				<data>abc</data>
				<data>def</data>
				<data>ghi</data>
			</list-data-provider>
			
			<setup>
				<s:collection-add collection="attr: testSuiteData" value="testCaseSetup-dataSetupFail"/>
			</setup>
			
			<data-setup>
				<s:assert-equals actual="int: 1" expected="int: 2"/>
				<s:collection-add collection="attr: testSuiteData" value="dataSetup-dataSetupFail"/>
			</data-setup>
		
			<s:log message="dataSetupFail - ${attr.dataString}"/>
			<s:collection-add collection="attr: testSuiteData" value="testCase-${attr.dataString}"/>
			
			<data-cleanup>
				<s:log message="dataSetupFail - data-cleanup"/>
				<s:collection-add collection="attr: testSuiteData" value="dataCleanup-dataSetupFail"/>
			</data-cleanup>

			<cleanup>
				<s:log message="dataSetupFail - cleanup"/>
				<s:collection-add collection="attr: testSuiteData" value="testCaseCleanup-dataSetupFail"/>
			</cleanup>
		</testCase>

		<testCase name="dataCleanupFail">
			<description>
				Validates the use case when data setup fails.
			</description>
			
			<list-data-provider name="dataString">
				<data>abc</data>
				<data>def</data>
				<data>ghi</data>
			</list-data-provider>
			
			<setup>
				<s:collection-add collection="attr: testSuiteData" value="testCaseSetup-dataCleanupFail"/>
			</setup>
			
			<data-setup>
				<s:collection-add collection="attr: testSuiteData" value="dataSetup-dataCleanupFail"/>
			</data-setup>
		
			<s:log message="dataCleanupFail - ${attr.dataString}"/>
			<s:collection-add collection="attr: testSuiteData" value="testCase-${attr.dataString}"/>
			
			<data-cleanup>
				<s:log message="dataCleanupFail - data-cleanup"/>
				<s:assert-equals actual="int: 1" expected="int: 2"/>
				<s:collection-add collection="attr: testSuiteData" value="dataCleanup-dataCleanupFail"/>
			</data-cleanup>

			<cleanup>
				<s:log message="dataCleanupFail - cleanup"/>
				<s:collection-add collection="attr: testSuiteData" value="testCaseCleanup-dataCleanupFail"/>
			</cleanup>
		</testCase>

	</testSuite>
</testData>