<testData xmlns:s="http://autox.yukthitech.com/steps" 
		xmlns:wrap="http://xmlbeanparser.yukthitech.com/wrap" 
		xmlns:f="http://autox.yukthitech.com/functions">
		
	<function name="function1">
		<s:log message="Function function1"/>
	</function>

	<testSuite name="test-suite-1">
		<setup>
			<s:log message="Setup of test-suite-1"/>
			<s:collection-add collection="attr: testSuite1Flow" value="testSuiteSetup"/>
		</setup>
		
		<before-test-case>
			<s:log message="before test-case of test-suite-1"/>
			<s:collection-add collection="attr: testSuite1Flow" value="beforeTestCase"/>
		</before-test-case>
		
		<after-test-case>
			<s:log message="after test-case of test-suite-1"/>
			<s:collection-add collection="attr: testSuite1Flow" value="afterTestCase"/>
		</after-test-case>

		<testCase name="testCase1">
			<description>
				Test case 1
			</description>
			
			<setup>
				<s:log message="Setup of Test case 1"/>
				<s:collection-add collection="attr: testSuite1Flow" value="testCase1Setup"/>
			</setup>
			
			<s:log message="Test case 1 - 1"/>
			<f:function1/>
			<s:log message="Test case 1 - 2"/>
			<s:collection-add collection="attr: testSuite1Flow" value="testCase1"/>
			
			<cleanup>
				<s:log message="Cleanup of Test case 1"/>
				<s:collection-add collection="attr: testSuite1Flow" value="testCase1Cleanup"/>
			</cleanup>
		</testCase>
	
		<testCase name="testCase2">
			<description>
				Test case 2
			</description>
			
			<setup>
				<s:log message="Setup of Test case 2"/>
				<s:collection-add collection="attr: testSuite1Flow" value="testCase2Setup"/>
			</setup>
			
			<s:log message="Test case 2"/>
			<s:collection-add collection="attr: testSuite1Flow" value="testCase2"/>
			
			<cleanup>
				<s:log message="Cleanup of Test case 2"/>
				<s:collection-add collection="attr: testSuite1Flow" value="testCase2Cleanup"/>
			</cleanup>
		</testCase>

		<cleanup>
			<s:log message="Cleanup of test-suite-1"/>
			<s:collection-add collection="attr: testSuite1Flow" value="testSuiteCleanup"/>
		</cleanup>
	</testSuite>
</testData>